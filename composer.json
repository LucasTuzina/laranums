{
    "name": "lucastuzina/laranums",
    "description": "Provides a trait with useful utility methods for enums in Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Lucastuzina\\Laranums\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lucastuzina\\Laranums\\LaranumsServiceProvider"
            ]
        }
    },
    "require-dev": {
        "illuminate/console": "^12.2",
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0 || ^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Lucastuzina\\Laranums\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
